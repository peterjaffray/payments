<script src="https://www.choice.marketing/assets/built/casper.js"></script>
<script>
    $(document).ready(function () {
        // Mobile Menu Trigger
        $('.gh-burger').click(function () {
            $('body').toggleClass('gh-head-open');
        });
       
    });
</script>

<script>
    // Delay pixel fire by 3 seconds
    setTimeout(function() {
        fbq('trackCustom', 'TimeOnPage', {duration: 3})
    }, 2222);
</script>

